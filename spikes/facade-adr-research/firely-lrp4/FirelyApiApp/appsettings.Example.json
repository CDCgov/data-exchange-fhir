{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "UseLocalDevFolder": true,
  "AWS": {
    "Region": "us-east-1",
    "ServiceURL": "https://s3.amazonaws.com",
    "AccessKey": "YOUR_ACCESS_KEY",
    "SecretKey": "YOUR_SECRET_KEY",
    "BucketName": "YOUR_BUCKET_NAME"
  }
}
